{
  "name": "retrieval-graph",
  "version": "0.0.1",
  "description": "A starter template containing an example retrieval agent.",
  "main": "src/retrieval_graph/graph.ts",
  "author": "Your Name",
  "license": "MIT",
  "private": true,
  "type": "module",
  "packageManager": "yarn@1.22.22",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "dev": "npx @langchain/langgraph-cli dev",
    "server": "ts-node --esm src/server.ts",
    "server:dev": "ts-node --esm --watch src/server.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --ci --coverage --watchAll=false",
    "test:agent": "node --experimental-vm-modules node_modules/jest/bin/jest.js src/__tests__/agent.test.ts",
    "test:int": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=\\.int\\.test\\.ts$",
    "lint": "eslint src",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@elastic/elasticsearch": "^8.15.0",
    "@langchain/anthropic": "^0.3.21",
    "@langchain/cohere": "^0.3.3",
    "@langchain/community": "^0.3.45",
    "@langchain/core": "^0.3.57",
    "@langchain/langgraph": "^0.3.0",
    "@langchain/langgraph-sdk": "^0.1.1",
    "@langchain/mongodb": "^0.1.0",
    "@langchain/openai": "^0.5.11",
    "@langchain/pinecone": "^0.2.0",
    "@pinecone-database/pinecone": "^5.1.2",
    "langchain": "^0.3.27",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "mongodb": "^6.9.0",
    "ts-node": "^10.9.2",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.1.0",
    "@eslint/js": "^9.9.1",
    "@jest/globals": "^29.7.0",
    "@tsconfig/recommended": "^1.0.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-no-instanceof": "^1.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.3"
  }
