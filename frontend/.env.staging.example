# Chef Chopsky Frontend Environment Variables - Staging Environment

# Supabase Configuration (Required)
# Use your staging Supabase project
NEXT_PUBLIC_SUPABASE_URL=https://your-staging-project-ref.supabase.co
NEXT_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your_staging_publishable_key_here

# Supabase Secret Key (Required for server-side operations)
SUPABASE_SECRET_KEY=your_staging_supabase_secret_key_here

# Agent Service Configuration (Required)
# Point to your staging agent service
AGENT_SERVICE_URL=https://your-agent-staging-url.railway.app

# Environment Configuration
NODE_ENV=production
NEXT_PUBLIC_APP_ENV=staging

# Environment Discriminator (should match agent APP_ENV)
APP_ENV=staging

# Staging-specific settings
NEXT_PUBLIC_DEBUG_MODE=false
NEXT_PUBLIC_LOG_LEVEL=info

# Analytics and Monitoring (Optional)
NEXT_PUBLIC_ANALYTICS_ID=your_staging_analytics_id
NEXT_PUBLIC_SENTRY_DSN=your_staging_sentry_dsn
